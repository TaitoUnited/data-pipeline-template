#!/usr/local/bin/python3
import sys
from src.worker import start_worker
from src.example.etl import sale_etl


command = sys.argv[-1]

if command == 'start_worker':
    start_worker(True)
if command == 'sales_listen':
    sale_etl.listen()
elif command == 'sales_extract':
    sale_etl.extract()
else:
    print("Unknown command: " + command, file=sys.stderr)
    exit(1)
